{
  "compilerOptions": {
    "jsx": "preserve",
    "target": "es2017",
    "lib": [
      "dom",
      "esnext"
    ],
    "module": "esnext",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true
    ,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    ".next/types/**/*.ts"
, "src/app/actions/getRedirectTarget"  ],
  "exclude": [
    "node_modules"
  ]
}
